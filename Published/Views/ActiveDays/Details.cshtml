@model Marksheet.Models.ActiveDays

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="card mb-4 col-md-8">
    <div class="card-header py-3">
        <h3 class="m-1 font-weight-bold center text-primary">Active Days</h3>
    </div>

    <div style="padding-top:10px;">
        <table class="table table-bordered" id="myTable">
            <tr>
                <th>
                   Fiscal Year
                </th>
                <td>


                    @Html.DisplayFor(model => model.AcademicYear.FiscalYear)
                </td>
            </tr>

            <tr>
                <th>
                  School Name
                </th>
                <td>


                    @Html.DisplayFor(model => model.School.SchoolName)
                </td>
            </tr>

            <tr>
                <th>
                   Terminal
                </th>
                <td>


                    @Html.DisplayFor(model => model.TerminalName)
                </td>
            </tr>

            <tr>
                <th>
                   Active Days
                </th>
                <td>


                    @Html.DisplayFor(model => model.Activeday)
                </td>
            </tr>

        </table>
    </div>
    <p>
       
        @Html.ActionLink("Back to List", "Index", new { },new { @class="btn btn-primary"})
    </p>
</div>
