<body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">
            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="@Url.Action("Index","Schools")">
                <div>
                    <img src="~/staticimg/school.jpg" height="42" width="42" />
                </div>
                <div class="sidebar-brand-text mx-3">School <sup></sup></div>
            </a>
            <!-- Divider -->
            <hr class="sidebar-divider my-0">
            <!-- Nav Item - Dashboard -->


            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","Schools")">
                    <i class="fas fa-school"></i>
                    <span>Schools</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","Students")">
                    <i class="fas fa-users"></i>
                    <span>Students</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","Subjects")">
                    <i class="fas fa-book"></i>
                    <span>Subjects</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","ActiveDays")">
                    <i class="fas fa-calendar-day"></i>
                    <span>Active Days</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","AcademicYears")">
                    <i class="fas fa-calendar-check"></i>
                    <span>Academic Years</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","Account")">
                    <i class="fas fa-user-tie"></i>
                    <span>Account</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","OptionalSubjects")">
                    <i class="fas fa-book-reader"></i>
                    <span>Optional subjects</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapsePages1" aria-expanded="true" aria-controls="collapsePages1">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>Marksheet</span>
                </a>
                <div id="collapsePages1" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">

                        <a class="collapse-item" href="@Url.Action("SelectSchool","Marksheet")">Marksheet Entry</a>
                        <a class="collapse-item" href="@Url.Action("SelectSchoolEdit","Marksheet")">Marksheet Edit</a>
                        <a class="collapse-item" href="@Url.Action("Index","Marksheet")">View Result</a>
                        <a class="collapse-item" href="@Url.Action("SelectSchoolPrint","Marksheet")">Print Schools Result</a>

                    </div>
                </div>
            </li>




            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">
            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>
